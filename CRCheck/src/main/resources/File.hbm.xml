<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.File" table="file" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="path">
            <column name="path" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="lastTime">
            <column name="lastTime" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>