<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Projectquality" table="projectquality" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="projectId">
            <column name="projectId" sql-type="int(11)"/>
        </property>
        <property name="endTime">
            <column name="endTime" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)"/>
        </property>
        <property name="method1">
            <column name="method1" sql-type="double" precision="-1"/>
        </property>
        <property name="method2">
            <column name="method2" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>