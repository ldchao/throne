<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Invitement" table="invitement" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="projectId">
            <column name="projectId" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>