<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Summary" table="summary" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="projectId">
            <column name="projectId" sql-type="int(11)"/>
        </property>
        <property name="location">
            <column name="location" sql-type="varchar(255)"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(255)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)"/>
        </property>
        <property name="combination">
            <column name="combination" sql-type="varchar(255)"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>