<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Summary" table="summary" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="projectId">
            <column name="projectId" sql-type="int(11)"/>
        </property>
        <property name="newPersonalReviewId">
            <column name="newPersonalReviewId" sql-type="int(11)"/>
        </property>
        <property name="oldPersonalReviewId">
            <column name="oldPersonalReviewId" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>