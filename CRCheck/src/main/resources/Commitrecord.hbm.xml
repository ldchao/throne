<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.Commitrecord" table="commitrecord" schema="throne">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="projectId">
            <column name="projectId" sql-type="int(11)"/>
        </property>
        <property name="commitTime">
            <column name="commitTime" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="codeLine">
            <column name="codeLine" sql-type="int(11)"/>
        </property>
        <property name="reviewType">
            <column name="ReviewType" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="time">
            <column name="time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>